<%= rug_form_for(@voting, url: main_app.votings_path, method: "post") do |f| %>
	<%= render partial: "nav_menu" %>
	<% if @voting.confirm_step? %>
		<%= render partial: "code", locals: { f: f } %>
		<%= render partial: "confirmation" %>
	<% else %>
		<%= render partial: "election_part", locals: { f: f, election_part: @voting.current_election_part } %>
	<% end %>
	<%= render partial: "nav_buttons", locals: { f: f } %>
<% end %>