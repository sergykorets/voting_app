<%= rug_menu_for(@voter, name: "actions", path_base: "main_app.admin_voter") do |m| %>
	<%= m.index_item %>
	<%= m.new_item %>
	<%= m.show_item %>
	<%= m.edit_item %>
	<% if @voter && !@voter.new_record? %>
		<%= m.item t("headers.admin.voters.generate_code"), main_app.generate_code_admin_voter_path(@voter), icon: "key", method: :put %>
	<% end %>
	<%= m.destroy_item %>
<% end %>