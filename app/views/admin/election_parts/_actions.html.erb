<%= rug_menu_for(@election_part, name: "actions") do |m| %>
	<%= m.new_item nil, main_app.new_admin_election_election_part_path(@election) %>
	<% if @election_part && !@election_part.new_record? %>
		<%= m.show_item nil, main_app.admin_election_election_part_path(@election, @election_part) %>
		<%= m.edit_item nil, main_app.edit_admin_election_election_part_path(@election, @election_part) %>
		<%= m.destroy_item nil, main_app.admin_election_election_part_path(@election, @election_part) %>
	<% end %>
<% end %>