<%= rug_menu_for(@candidate_participation, name: "actions") do |m| %>
	<%= m.new_item nil, main_app.new_admin_candidate_candidate_participation_path(@candidate) %>
	<% if @candidate_participation && !@candidate_participation.new_record? %>
		<%= m.show_item nil, main_app.admin_candidate_candidate_participation_path(@candidate, @candidate_participation) %>
		<%= m.edit_item nil, main_app.edit_admin_candidate_candidate_participation_path(@candidate, @candidate_participation) %>
		<%= m.destroy_item nil, main_app.admin_candidate_candidate_participation_path(@candidate, @candidate_participation) %>
	<% end %>
<% end %>