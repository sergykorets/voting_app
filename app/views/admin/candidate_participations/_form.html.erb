<%= rug_form_for(@candidate_participation, create_url: "main_app.admin_candidate_participations_path", update_url: "main_app.admin_candidate_participation_path") do |f| %>

	<%= f.text_input_row(:votes, :number_field) %>
	<%= f.text_area_row(:description) %>
	<%= f.text_area_row(:visions) %>
	<%= f.text_input_row :candidate_id, :number_field %>
	<%= f.text_input_row :election_part_id, :number_field %>
	<%= f.primary_button_row :submit %>
	<%= f.back_link_button_row %>
<% end %>